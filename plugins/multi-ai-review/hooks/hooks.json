{
  "description": "Multi-AI review event hooks",
  "hooks": {
    "SubagentStop": [
      {
        "matcher": "multi-ai-review:review-coordinator",
        "hooks": [
          {
            "type": "command",
            "command": "echo 'Multi-AI review complete. Use /multi-ai-review:report to view detailed results.'"
          }
        ]
      }
    ]
  }
}
